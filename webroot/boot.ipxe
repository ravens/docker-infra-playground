#!ipxe

# Boot a persistent RancherOS to RAM

# Location of Kernel/Initrd images
set base-url http://releases.rancher.com/os/latest

kernel ${base-url}/vmlinuz rancher.password=rancher console=tty1 printk.devkmsg=on panic=10 rancher.cloud_init.datasources=[url:http://192.168.25.3/cloud-config]
initrd ${base-url}/initrd
boot

